        <Image x:Name="imgPricing" HorizontalAlignment="Left" Height="51" Margin="63,140,0,0" VerticalAlignment="Top" Width="50" Source="Images/pricing.png" Tag="2"/>
        <Label x:Name="lblPricingManagement" Content="Pricing Management" HorizontalAlignment="Left" Margin="135,151,0,0" VerticalAlignment="Top" Width="181" FontSize="20" FontWeight="Bold" FontFamily="Script MT Bold" IsHitTestVisible="False" Height="30" Foreground="#FF070708" Background="#00000000" TabIndex="3"/>
