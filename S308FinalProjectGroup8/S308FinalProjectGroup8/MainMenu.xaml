        <Label x:Name="lblExit" Content="Exit" HorizontalAlignment="Left" Margin="135,284,0,0" VerticalAlignment="Top" Width="219" FontSize="20" FontWeight="Bold" FontFamily="Script MT Bold" IsHitTestVisible="False" Height="30" Foreground="#FFD91025" Background="#00000000" Tag="7"/>
        <Image x:Name="imgExit" HorizontalAlignment="Left" Height="51" Margin="63,270,0,0" VerticalAlignment="Top" Width="50" Source="Images/Exit.png" Tag="6"/>
