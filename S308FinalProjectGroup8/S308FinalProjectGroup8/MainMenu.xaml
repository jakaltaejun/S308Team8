        <Label x:Name="lblMembershipSales" Content="Membership Sales" HorizontalAlignment="Left" Margin="135,89,0,0" VerticalAlignment="Top" Width="163" FontSize="20" FontWeight="Bold" FontFamily="Script MT Bold" IsHitTestVisible="False" Height="30" Foreground="#FF1369D6" Background="#00000000" TabIndex="1"/>
